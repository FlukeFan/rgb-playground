    <Compile Include="**\*.js" />
    <Compile Include="**\*.xaml" />
    <Compile Include="**\*.html" />
    <Compile Include="**\*.aspx" />
    <Compile Include="**\*.asax" />
    <Compile Include="**\*.master" />
    <Compile Include="**\*.css" />
    <Compile Include="**\*.ddl" />
    <Compile Include="**\*.dml" />
    <None Include="**\*.config" />
    <None Include="**\*.xml" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="mscorlib">
      <HintPath>SDKs\Ce\bin\mscorlib.dll</HintPath>
    </Reference>
    <Reference Include="System">
      <HintPath>SDKs\Ce\bin\System.dll</HintPath>
    </Reference>
    <Reference Include="System.Data">
